{
    "name": "unvurn/reauth",
    "description": "Reauth - Yet another authentication module for Laravel",
    "type": "library",
    "version": "0.1.1",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Unvurn\\Reauth\\": "src/"
        }
    },
    "authors": [
        {
            "name": "hironobu koura",
            "email": "hironobu@unvurn.cc"
        }
    ],
    "require": {
        "php": "^8.2",
        "ext-json": "*",
        "laravel/framework": "^11.0",
        "symfony/console": "^7.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.6",
        "phpstan/phpstan": "^1.11",
        "phpunit/phpunit": "^10.5"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Unvurn\\Reauth\\ReauthCommandServiceProvider",
                "Unvurn\\Reauth\\ReauthServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
